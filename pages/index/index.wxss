.pageview{
  width: 100%;
  height: 160rpx;
  font-size: 15px;
  margin-top: 15rpx;
}
.top{
  width: 100%;
  height: 30%;
}
.imgswiper{
  width: 100%;
  height: 100%;
}
.picture{
  width: 100%;
  height: 100%;
}
/*四个导航*/
.list{
  width: 100%;
  height: 15%;
}
.list>view{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.column{
  width: 20%;
  height: 80%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.icon{
  width: 80%;
  height: 100rpx;
  border-radius: 50%;
}
.icon>image{
  width: 100%;
  height: 100%;
}
.text{
  width: 80%;
  display: flex;
  justify-content: center;
  font-size: 30rpx;
  height: 50rpx;
  line-height: 50rpx;
  color: #999999;
}

.content{
  display: flex;  
  flex-direction: row;  
}
.wenhuaPic swiper{
  width: 750rpx;
  height: 340rpx;
}
.wenhuaPic image {
  margin-top: 20rpx;
  width: 100%;
}


input{
  border: 1px solid gray;
}
.line{
  margin: 0 auto;
  height: 1.5rpx;
  margin-top: 10rpx;
  margin-bottom: 5rpx;
  background-color: #D8D8D8;
}